# Pixiv Cookie (PHPSESSID) for authenticated requests (R18 content)
PIXIV_COOKIE=your_phpsessid_here

# Server port (default: 8000)
PORT=8000

# OAuth credentials (optional - defaults are built-in)
PIXIV_CLIENT_ID=your_client_id_here
PIXIV_CLIENT_SECRET=your_client_secret_here

# OAuth tokens (loaded on startup)
PIXIV_ACCESS_TOKEN=your_access_token_here
PIXIV_REFRESH_TOKEN=your_refresh_token_here
